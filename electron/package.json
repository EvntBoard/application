    "electron": "^10.1.2",
    "electron-builder": "^22.8.1",
    "electron-rebuild": "^2.0.3",
    "shelljs": "^0.8.4",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  }
